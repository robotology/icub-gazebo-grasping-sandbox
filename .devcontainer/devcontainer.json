{
  "name": "iCub Gazebo Grasping Sandbox",
  "image": "ghcr.io/robotology/icub-gazebo-grasping-sandbox:latest",
  "features": {
    "ghcr.io/devcontainers/features/github-cli:latest": { },
    "ghcr.io/devcontainers/features/git-lfs:latest": { }
  },
  "hostRequirements": {
    "cpus": 4
  },
  "forwardPorts": [6080],
  "portsAttributes": {
    "6080": {
      "label": "vnc",
      "onAutoForward": "openBrowserOnce"
    }        
  },
  "remoteUser": "codespace",
  "customizations": {
    "codespaces": {
      "openFiles": ["README.md"]
    }
  },
  "postAttachCommand": {
    "vnc": "start-vnc-session.sh"
  }
}
